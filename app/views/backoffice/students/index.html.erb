<h1 class="page-header"><%= @students.model_name.human(count: 2) %></h1>
<div class="panel panel-default">

  <div class="panel-heading">
    <%= t('messages.importing', model: @students.model_name.human(count: 2)) %>
  </div>
  <div class="panel-body">
    <%= form_tag import_backoffice_students_path, multipart: true do %>
      <%= file_field_tag :file, :class => 'btn btn-default' %>
      <p class="import_description">Importe um arquivo de extens√£o .xls ou .xlsx.</p>
      <br>
      <%= submit_tag t('buttons.import'), :class => 'btn btn-default'%>
    <% end %>
  </div>
</div>